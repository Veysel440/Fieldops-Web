{
  "ci": {
    "collect": {
      "url": ["http://localhost:4173/"],
      "numberOfRuns": 1
    },
    "assert": {
      "preset": "lighthouse:recommended",
      "assertions": {
        "uses-http2": "off",
        "csp-xss": "warn",
        "categories:pwa": "warn"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
